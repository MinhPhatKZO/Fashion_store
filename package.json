{
  "name": "fashion-store",
  "version": "1.0.0",
  "description": "Fashion Store E-commerce Application",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "build": "npm run build:web",
    "build:web": "cd web && npm run build",
    "install:all": "npm install && cd web && npm install && cd ../app && flutter pub get",
    "dev:all": "concurrently \"npm run dev\" \"cd web && npm start\"",
    "test": "jest",
    "seed": "node server/scripts/seed.js"
  },
  "keywords": [
    "ecommerce",
    "fashion",
    "react",
    "flutter",
    "express",
    "mongodb"
  ],
  "author": "Fashion Store Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.9.7",
    "react-toastify": "^11.0.5",
    "recharts": "^3.4.1",
    "stripe": "^14.7.0",
    "swiper": "^11.1.3"
  },
  "devDependencies": {
    "@types/react-toastify": "^4.1.0",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
